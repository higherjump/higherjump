<input id="kg" > your weight kg <br/>
<input    id="m" > the highest   meter <br/>
<input id="tall" > your height centimeter <br/>


  <div> your velocity at take off is <input id="v"> m/s    
 it is using your legs for lifting 
 <p id="thekg"> </p> kg for <input id="the"> of seconds 
</div>

 <br/><script type="text/javascript"> 
 
       
 var kg = document.getElementById('kg'). value ,
 m = document.getElementById('m'). value ,
tall = document.getElementById('tall') . value ,
v = Number ( ( 9.80665 * m * 2 ) ^ ( 1 / 2 ) ) ,

 


g = Number ( ( v^2 ) / ( m * 2 ) ) ,
 gg = Number ( ( g * m ) / ( tall / 6 ) ) 
 ,
   
 
   
 
thekg = Number  
( gg * kg / g ) ,

 
 
 
the = Number( v / ( ( ( tall / 6 ) * 2 * gg ) ^ ( 1 / 2 ) ) ) ;   
